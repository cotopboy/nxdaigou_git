<Application x:Class="daigou.main.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="XamlResources/BasicElements.xaml" />
                <ResourceDictionary Source="XamlResources/Converters.xaml" />
                <ResourceDictionary Source="XamlResources/StaticText.xaml"/>
                <ResourceDictionary Source="XamlResources/ControlStyles.xaml" />
                <ResourceDictionary Source="XamlResources/DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
